@model IEnumerable<KLTN.Models.Database.LichSuCheckIn>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ThoiGian)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.KetQuaNhanDien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.AnhNhanDien)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ThanhVien)
