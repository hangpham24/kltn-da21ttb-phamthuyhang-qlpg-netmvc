            <th>
                @Html.DisplayNameFor(model => model.KhachVangLai)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ThoiGian)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KetQuaNhanDien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.AnhNhanDien)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ThanhVien.MaTV)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.KhachVangLai.MaKVL)
            </td>
